# LightLane API Test Environment
# Copy this file to .env and fill in your values.
# The .env file is gitignored and will not be committed.

# API Base URL
API_BASE=http://127.0.0.1:1337

# Customer credentials (use a test account)
CUSTOMER_EMAIL=
CUSTOMER_PASSWORD=

# JWT token - copy from login response and paste here
CUSTOMER_TOKEN=

# Entitlement ID - copy from /api/customers/me/entitlements response
ENTITLEMENT_ID=

# Device identifiers for testing (can leave as defaults)
DEVICE_ID_A=dev_A_001
DEVICE_ID_B=dev_B_001
PUBLIC_KEY_A=pk_test_A_0000000000000000000000000000
PUBLIC_KEY_B=pk_test_B_0000000000000000000000000000
