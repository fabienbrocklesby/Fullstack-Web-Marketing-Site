{
  "name": "lightlane",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "pnpm -r --parallel --filter ./apps/** dev",
    "build": "pnpm -r --filter ./apps/** build",
    "start": "pm2 start ecosystem.config.js && pm2 logs",
    "lint": "pnpm -r lint"
  }
}
